.search-results
  list-style none
  padding    0

.search-result
  display        block
  padding        3px 6px

  font-family    sans-serif
  font-size      14px
  text-transform uppercase
  letter-spacing -1px
  text-shadow    0 1px 0 black

  cursor pointer

  &:hover
    background rgba(0,0,0,0.5)
